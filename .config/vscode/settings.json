{
  "git.ignoreMissingGitWarning": true,
  "explorer.confirmDragAndDrop": false,
  "markdown.preview.breaks": true,
  "workbench.editor.enablePreview": false,
  "workbench.colorTheme": "One Dark Pro",
  "workbench.editor.enablePreviewFromQuickOpen": false,
  "workbench.iconTheme": "vscode-icons",
  "workbench.startupEditor": "newUntitledFile",
  "workbench.commandPalette.history": 100,
  "workbench.activityBar.visible": true,
  "workbench.editor.closeOnFileDelete": true,
  "workbench.enableExperiments": false,
  "workbench.settings.editor": "json",
  "workbench.settings.enableNaturalLanguageSearch": false,
  "workbench.statusBar.feedback.visible": false,
  "workbench.tips.enabled": false,
  "breadcrumbs.enabled": false,
  "diffEditor.renderSideBySide": false,
  "editor.cursorStyle": "line-thin",
  "editor.formatOnSave": true,
  "editor.glyphMargin": false,
  "editor.letterSpacing": 0.5,
  "editor.lineHeight": 18,
  "emmet.includeLanguages": {
    "vue-html": "html",
    "javascript": "javascriptreact"
  },
  "emmet.triggerExpansionOnTab": true,
  "explorer.decorations.colors": false,
  "extensions.autoUpdate": false,
  "extensions.ignoreRecommendations": true,
  "files.associations": {
    "*.js": "javascriptreact",
    "*.html": "html"
  },
  "git.allowForcePush": true,
  "git.autofetch": true,
  "git.confirmForcePush": false,
  "git.confirmSync": false,
  "javascript.format.enable": false,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "javascript.validate.enable": false,
  "keyboard.touchbar.enabled": false,
  "npm.runSilent": true,
  "prettier.disableLanguages": ["javascript", "javascriptreact"],
  "search.showLineNumbers": true,
  "search.useGlobalIgnoreFiles": true,
  "telemetry.enableCrashReporter": false,
  "terminal.integrated.lineHeight": 1.15,
  "typescript.format.enable": false,
  "typescript.surveys.enabled": false,
  "typescript.validate.enable": false,
  "update.enableWindowsBackgroundUpdates": false,
  "update.showReleaseNotes": false,
  "window.closeWhenEmpty": true,
  "window.restoreFullscreen": true,
  "gitlens.advanced.messages": {
    "suppressShowKeyBindingsNotice": true
  },
  "editor.overviewRulerLanes": 6,
  "window.zoomLevel": 0,
  "editor.fontFamily": "Knack Nerd Font, Source Code Pro, Menlo, Monaco, 'Courier New', monospace",
  "editor.fontSize": 12,
  "editor.tabSize": 2,
  "editor.rulers": [80, 120],
  "editor.minimap.enabled": false,
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/*.pyc": true
  },
  "files.trimTrailingWhitespace": true,
  "telemetry.enableTelemetry": false,
  "files.autoSave": "onFocusChange",
  "files.autoSaveDelay": 100,
  "terminal.external.osxExec": "iTerm.app",
  "editor.formatOnPaste": true,
  "editor.quickSuggestions": {
    "strings": true
  },
  "bookmarks.gutterIconPath": "",
  "workbench.quickOpen.closeOnFocusLost": false,
  "explorer.sortOrder": "default"
  // "vim.disableAnnoyingNeovimMessage": true,
  // "vim.hlsearch": true,
  // "vim.incsearch": true,
  // "vim.leader": "<space>",
  // "vim.statusBarColorControl": true,
  // "vim.useCtrlKeys": true,
  // "vim.useSystemClipboard": true,
}
